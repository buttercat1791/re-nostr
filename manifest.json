  "manifest_version": 3,
  "icons": {
    "512": "./src/assets/icon/icon.png"
  },
  "action": {
    "default_popup": "./src/index.html",
    "default_title": "Re:Nostr"
  },
  "permissions": [
    "activeTab"
  ]
}
