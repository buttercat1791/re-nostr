{
  "name": "Re:Nostr",
  "description": "Send notes and other stuff to Nostr from anywhere around the web",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Re:Nostr"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": [{
    "src": "assets/icon/icon.png",
    "sizes": "512x512",
    "type": "image/png"
  }],
  "background_color": "#16161d",
  "theme_color": "#16161d"
}
