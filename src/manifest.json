{
  "name": "Re:Nostr",
  "description": "Send notes and other stuff to Nostr from anywhere around the web",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": [{
    "512": "assets/icon/icon.png"
  }],
  "action": {
    "default_popup": "index.html",
    "default_title": "Re:Nostr"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "browser-polyfill.js"
    ]
  }
}
